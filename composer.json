{
    "name": "rohsyl/salto",
    "description": "A PHP Salto client implementing the PMS Industry Standard protocol via TCP/IP.",
    "keywords": [
        "salto",
        "php"
    ],
    "homepage": "https://github.com/rohsyl/salto",
    "license": "MIT",
    "authors": [
        {
            "name": "Sylvain Roh",
            "email": "syzin12@gmail.com",
            "homepage": "https://rohs.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-sockets": "*",
        "nesbot/carbon": "^2.62"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "rohsyl\\Salto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rohsyl\\Salto\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "rohsyl\\Salto\\SaltoServiceProvider"
            ],
            "aliases": {
                "Salto": "rohsyl\\Salto\\Salto"
            }
        }
    }
}
